<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  
  # SPA Routes
  # /market
  # /profile
  # /portfolio
  # /dashboard
  # /transactions
  # /calculator
  
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-l
  RewriteRule . /index.html [L]
</IfModule>
